.m-part-2
	width 100%
	background-color GRAY_E3
	background-image url('images/hi-bg-page3.png')
	background-repeat repeat
	padding 20px 0px 50px 0px
	display block
	overflow hidden
	.m-part-2-container
		width 100%
		height 471px
		overflow hidden
		display block
		position relative
		.m-part-2-item
			width 200px
			height 471px
			position absolute
			left 50%
			top 0px
			margin-left -100px
			img
				width 100%
				height 100%
				display block
				float left
		.m-part2-arrowleft
			position absolute
			width 45px
			height 70px
			background-image url('images/m-part2-arrowleft.png')
			background-repeat no-repeat
			background-position center center
			left 5px
			z-index 10
			top 50%
			margin-top -35px
		.m-part2-arrowright
			position absolute
			width 45px
			height 70px
			background-image url('images/m-part2-arrowright.png')
			background-repeat no-repeat
			background-position center center
			right 5px
			z-index 10
			top 50%
			margin-top -35px