.page-3-content
	width auto
	height auto
	position absolute
	left 0%
	top 0%
	overflow hidden
	display block
	.page-title
		position static
		display block
		width auto
		margin-bottom 10px
		margin-left 20px
		@media HEIGHT_LARGE
			margin-left 16px
		@media HEIGHT_SMALL
			margin-left 5px
		@media WIDTH_THIN
			margin-left 5px
	.page-3-item
		width 200px
		height 69px
		margin 25px 15px
		float left
		box-sizing border-box
		border 1px solid #f9f9f9
		background-image url('images/hi-bg-white-pct60.png')
		background-repeat repeat
		&:hover
			border-color #ffffff
		&.page-3-item-small
			width 130px
		img
			width 100%
			height 100%
			display block
			float left
		@media HEIGHT_LARGE
			width 260px
			height 90px
			margin 32px 16px
			&.page-3-item-small
				width 188px
		@media HEIGHT_SMALL
			margin 5px
		@media WIDTH_THIN
			margin 5px

